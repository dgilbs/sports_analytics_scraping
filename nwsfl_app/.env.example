# Database connection string
# Copy this file to .env and fill in your actual database credentials
DATABASE_URL=postgresql://username:password@host:port/database?sslmode=require

# dbt Connection Parameters
DBT_HOST=your_database_host
DBT_USER=your_database_user
DBT_PASSWORD=your_database_password
DBT_PORT=5432
DBT_DBNAME=your_database_name
